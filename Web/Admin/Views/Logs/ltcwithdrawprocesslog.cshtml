@{
    Layout = null;
}
<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title" >用户操作日志</h4>
    </div>
    <div class="panel-body">
        <div class="pull-left">

        </div>
        <div class="pull-right padding-right100">
            <label>用户Email:</label><input type="text" class="form-control inline-control " ng-model="Email_Search">
            <button type="button" class="btn btn-success" ng-click="search()"><span class="glyphicon glyphicon-search"></span>搜索</button>
        </div>
    </div>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>#</th>
                <th>ID</th>
                <th>用户Email</th>
                <th>操作员Email</th>
                <th>操作备注</th>
                <th>操作时间</th>
            </tr>
        </thead>
        <tbody ng-show="logss">
            <tr ng-repeat="logs in logss">
                <td><input type="checkbox" ng-model="logs.Checked" ng-change="checkedChange(logs)"></td>
                <td ng-bind="logs.ID"></td>
                <td ng-bind="logs.DomainEmail"></td>
                <td ng-bind="logs.OperatorEmail"></td>
                <td ng-bind="logs.Memo"></td>
                <td ng-bind="logs.OperateTime*1000 | date:'yyyy-MM-dd HH:mm:ss'"></td>
            </tr>
        </tbody>
    </table>
    <pagination total-items="totalCount" page="currentPage" first-text="首页" on-select-page="pageChange(page)" next-text="下一页" previous-text="上一页" last-text="尾页" max-size="10" class="pagination-sm" boundary-links="true" rotate="false" num-pages="numPages"></pagination>
    <pre>共{{totalCount}}条数据  第{{currentPage}}页 /共{{numPages}}页</pre>
</div>
