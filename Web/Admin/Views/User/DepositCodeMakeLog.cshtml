@{
    Layout = null;
}
<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title">客服&nbsp;&nbsp;{{nickname}}&nbsp;&nbsp;充值码使用日志</h4>
    </div>
    <div class="panel-body">
        <div class="pull-right padding-right100">
            <label>开始时间:</label><input type="text" class="form-control inline-control " ng-model="start_Search" onclick="WdatePicker()" onchange="">
            <label>结束时间:</label><input type="text" class="form-control inline-control " ng-model="end_Search" onclick="WdatePicker()" onchange="">
            <button type="button" class="btn btn-success" ng-click="search()"><span class="glyphicon glyphicon-search"></span>搜索</button>
        </div>
    </div>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>操作时间</th>
                <th>备注信息</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="userLog in userLogs">
                <td><span ng-bind="userLog.ID"></span></td>  
                <td ng-bind="userLog.OperateTime*1000 | date:'yyyy-MM-dd HH:mm:ss'"></td>
                <td ng-bind="userLog.Memo"></td>
            </tr>
        </tbody>
    </table>
    <pagination total-items="totalCount" page="currentPage" first-text="首页" next-text="下一页" previous-text="上一页" last-text="尾页" max-size="10" class="pagination-sm" boundary-links="true" rotate="false" num-pages="numPages"></pagination>
</div>

