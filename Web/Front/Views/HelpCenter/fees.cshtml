    <script type="text/javascript">
        function cancelOrderOneCurrency(event) {
            var m_base = $(event.srcElement).attr("baseCurrency");
            var m_target = $(event.srcElement).attr("targetCurrency");
            var orderID = $(event.srcElement).attr("data_id");
            $.post("/cancelOrder", { orderID: orderID, base: m_base, target: m_target }, function (data, status) {
                if (data.Code == 1) {
                    $(event.srcElement).parents("tr").remove();
                    noty({ text: MSG_Cancel_Order_Success, maxVisible: 30, type: "success", layout: "bottom", timeout: 5000 });
                }
                else if (data.Code == -1) {
                    noty({ text: MSG_Cancel_Order_Done, maxVisible: 30, type: "success", layout: "bottom", timeout: 5000 });
                } else if (data.Code == -2) {
                    noty({ text: MSG_Cancel_Order_Canceled, maxVisible: 30, type: "information", layout: "bottom", timeout: 5000 });
                }
            });
        }
        $(document).ready(function () {

            // Setup the ticket grid
            ticketGrid = new RichHTML.grid({
                el: 'ticketGrid',
                tableclass: 'table',
                url: '/action/LoginHistory',
                emptyText: 'There are no withdraws to display',
                baseParams: {
                    limit: 20,
                    start: 0,
                },
                root: 'data',
                totalProperty: 'totalCount',
                columns: [
                {
                    text: "登录时间",
                    dataIndex: "LoginTime",
                    align: "center",
                    sortable: false,
                    renderer: function (text, row, el) {
                        return new Date(row.LoginTime * 1000);
                    }
                },
                {
                    text: "登录地址",
                    dataIndex: "IP",
                    align: "center",
                    sortable: false
                }]
            });

            // Bind to some grid events
            $(ticketGrid).bind({
                // Loading data
                "load": function (event, data) {
                    if (data.paging.start == null) {
                        var paging = "Displaying withdraws 0 - 0 of 0<br />";
                    } else {
                        var paging = "Displaying withdraws " + (data.paging.start + 1) + " - " + (data.paging.start + data.paging.items) + " of " + data.paging.totalItems + "<br />";
                    }
                    // Setup the paging information
                    $('#tablemetadata').html(paging);
                }
            });

            ticketGrid.render();

        });
    </script>
<div id="tablemetadata"></div>
<div id="ticketGrid"></div>
 

